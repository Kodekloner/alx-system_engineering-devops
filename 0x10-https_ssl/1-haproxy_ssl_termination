defaults
  mode http
  timeout client 15s
  timeout connect 10s
  timeout server 15s
  timeout http-request 10s
frontend kodekloner-frontend
    bind *:80 ssl crt /etc/letsencrypt/live/www.kodekloner.tech/www.kodekloner.tech.pem
    default_backend kodekloner-backend
backend kodekloner-backend
    balance roundrobin
    server 437249-web-01 54.226.9.101:80 check
    server 437249-web-02 34.232.53.161:80 check
